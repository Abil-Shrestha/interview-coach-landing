{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/app/:path*",
      "destination": "https://app.tryinterviewcoach.com/:path*",
      "permanent": false
    },
    {
      "source": "/login",
      "destination": "https://app.tryinterviewcoach.com/login",
      "permanent": false
    },
    {
      "source": "/signup",
      "destination": "https://app.tryinterviewcoach.com/login",
      "permanent": false
    }
  ]
}
